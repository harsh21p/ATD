<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <ImageView
        android:id="@+id/imageView11_garage"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/base_shape_to_hold_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ellipse" />

    <ImageView
        android:id="@+id/iv_icon_account"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:contentDescription="@string/icon_of_respective_preference_setting"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11_garage"
        app:layout_constraintEnd_toEndOf="@+id/imageView11_garage"
        app:layout_constraintStart_toStartOf="@+id/imageView11_garage"
        app:layout_constraintTop_toTopOf="@+id/imageView11_garage"
        app:srcCompat="@drawable/logout_icon" />

    <TextView
        android:id="@+id/tv_menu_item_name_garage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/dummyText"
        android:textColor="@color/atd_dark_blue"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11_garage"
        app:layout_constraintStart_toEndOf="@+id/imageView11_garage"
        app:layout_constraintTop_toTopOf="@+id/imageView11_garage" />

    <ImageView
        android:id="@+id/imageView13_garage"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="@string/icon_for_arrow_right"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11_garage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView11_garage"
        app:srcCompat="@drawable/right_arrow_atd" />

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginTop="8dp"
        android:background="@color/lightgreay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11_garage" />



</androidx.constraintlayout.widget.ConstraintLayout>